<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo.mapper.PassForgetMapper">

    
    
    <select id="selectEmail" parameterType="com.example.demo.model.PassForgetModel" >
    	SELECT email FROM visalogintable WHERE email=#{email}  ;		
    </select>
    
    <update id="updatePass" parameterType="com.example.demo.model.PassForgetModel" >
    	UPDATE	visalogintable SET password = #{password} WHERE email=#{email}
    </update>
    
    
   
</mapper>



